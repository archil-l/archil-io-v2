{
  "name": "homepage-assistant-ui",
  "version": "1.0.0",
  "description": "Homepage Assistant UI with React Router v7 and AWS CDK deployment",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npx @react-router/dev build",
    "release": "rm -rf dist && npm run build && mkdir -p dist/lambda-pkg && cp -r dist/server dist/lambda-pkg/ && node esbuild.config.js",
    "dev": "npx @react-router/dev dev",
    "start": "npx @react-router/serve ./dist/server/index.js",
    "clean": "rm -rf dist node_modules",
    "cdk:build": "tsc -p cdk/tsconfig.json"
  },
