  "scripts": {
    "typecheck": "npx tsc --noEmit",
    "build": "npx @react-router/dev build",
    "release": "rm -rf dist && npm run build && mkdir -p dist/lambda-pkg dist/streaming-lambda && node esbuild.config.js",
    "release:streaming": "mkdir -p dist/streaming-lambda && node esbuild.config.js",
    "dev": "npx @react-router/dev dev",
    "start": "npx @react-router/serve ./dist/server/index.js",
    "clean": "rm -rf dist node_modules",
    "cdk:build": "tsc -p cdk/tsconfig.json",
    "cdk:deploy:streaming": "npm run release:streaming && npm run cdk:build && npx cdk deploy archil-io-v2-llm-stream-prod --require-approval never"
  },
